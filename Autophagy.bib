@Article{Liu2017a,
  author               = {Liu, Yanli and Xu, Hanchun and An, Meixia},
  journal              = {Scientific reports},
  title                = {mTORC1 regulates apoptosis and cell proliferation in pterygium via targeting autophagy and FGFR3.},
  year                 = {2017},
  issn                 = {2045-2322},
  month                = aug,
  pages                = {7339},
  volume               = {7},
  abstract             = {Pterygium is one of the most common ocular surface diseases. During the initiation of pterygium, resting epithelial cells are activated and exhibit aberrant apoptosis and cell proliferation. Mechanistic target of rapamycin complex 1 (mTORC1) is a central regulator of cell growth, cell proliferation, protein synthesis, autophagy and transcription. However, the effect of mTORC1 activation in epithelial cells on pterygium development has not yet been reported. Additionally, the roles of mTORC1 in aberrant apoptosis and cell proliferation during the initiation of pterygium, and the underlying mechanisms, are not known. Herein, we evaluated mTOR signalling in pterygium growth and development. The results revealed that mTOR signalling, especially mTORC1 signaling, is highly activated, and aberrant apoptosis and cell proliferation were observed in pterygium. mTORC1 activation inhibits apoptosis in pterygium by regulating Beclin 1-dependent autophagy via targeting Bcl-2. mTORC1 also negatively regulates fibroblast growth factor receptor 3 (FGFR3) through inhibition of p73, thereby stimulating cell proliferation in pterygium. These data demonstrate that mTORC1 signalling is highly activated in pterygium and provide new insights into the pathogenesis and progression of pterygium. Hence, mTORC1 may be a novel therapeutic target for the treatment of pterygium.},
